{
  "name": "generate-strapi-typescript",
  "version": "0.1.2",
  "description": "Package to generate typescript interfaces for strapi v4 components and collections",
  "main": "index.js",
  "type": "module",
  "files": [
    "src/*",
    "index.js",
    "Readme.md",
    "package.json"
  ],
  "bin": {
    "gst": "index.js"
  },
  "engines": {
    "node": "^12"
  },
  "scripts": {
    "start": "node ./index.js",
    "lint": "eslint index.js ./src/*.js",
    "lint:fix": "npm run lint -- --fix",
    "test": "node index.js ../psykoty/cms/app/src ./types.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuli00/generate-strapi-typescript.git"
  },
  "keywords": [
    "strapi",
    "v4",
    "typescript",
    "interface",
    "types",
    "ts"
  ],
  "author": "Maciej Kuli≈Ñski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kuli00/generate-strapi-typescript/issues"
  },
  "homepage": "https://github.com/kuli00/generate-strapi-typescript#readme",
  "devDependencies": {
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.6.0"
  }
}
